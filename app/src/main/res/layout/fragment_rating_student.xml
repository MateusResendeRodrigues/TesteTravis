<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayoutRating"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textViewNameProfessorRating"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Nome"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textViewSubjectProfessorRating"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Disciplina"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textViewLevelProfessorRating"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Level"
                tools:ignore="MissingConstraints" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="111dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">



                <Button
                    android:id="@+id/buttonVeryGood"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:text="Muito Bom"
                    android:textColor="@android:color/background_light"
                    android:theme="@style/button" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="MissingConstraints,VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="MissingConstraints,VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="MissingConstraints,VectorDrawableCompat" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/buttonGood"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:text="Bom"
                    android:textColor="@android:color/background_light"
                    android:theme="@style/button" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="ContentDescription,MissingConstraints,VectorDrawableCompat"
                    tools:layout_editor_absoluteX="176dp"
                    tools:layout_editor_absoluteY="175dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/buttonMedio"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:text="MÃ©dio"
                    android:textColor="@android:color/background_light"
                    android:theme="@style/button" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_star"
                    tools:ignore="MissingConstraints,VectorDrawableCompat" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/buttonBad"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:text="Ruim"
                    android:textColor="@android:color/background_light"
                    android:theme="@style/button" />

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="38dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/ic_menu_dislike"
                    tools:ignore="MissingConstraints,VectorDrawableCompat" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>



</android.support.constraint.ConstraintLayout>